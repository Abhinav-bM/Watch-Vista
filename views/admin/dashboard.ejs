    <script>
        // SALES DATA CHART
        const ctx = document.getElementById('myChart');

        function getSalesData() {
            const salesDataInput = document.getElementById('salesData');
            if (salesDataInput) {
                const salesData = JSON.parse(salesDataInput.value);
                return salesData;
            }
            return []
        }
        
        const salesData = getSalesData();
        const chartLabels = salesData.map(item => item.date);
        const chartValues = salesData.map(item => item.totalSales);
        
        new Chart(ctx, {
            type: 'bar',
            data: {
                labels: chartLabels,
                datasets: [{
                    label: 'Sales',
                    data: chartValues,
                    backgroundColor: ['rgb(255, 99, 132)']
                }]
            },
            options: {
                scales: {
                    y: { beginAtZero: true }
                }
            }
        });


        // CHART ORDERS
        const ctx2 = document.getElementById('myChartOrders');
        function getOrderData (){
            const orderDataInput = document.getElementById('orderData')
            if(orderDataInput){
                const orderData  = JSON.parse(orderDataInput.value)
                return orderData
            }
            return []
        }
        const orderData = getOrderData()
        const orderDataLabels = []
        const orderDataValues = []

        Object.keys(orderData).forEach((date) => {
            orderDataLabels.push(date);
            orderDataValues.push(orderData[date]);
        });
        orderDataLabels.reverse()
        orderDataValues.reverse()

        new Chart(ctx2, {
            type: 'pie',
            data: {
                labels: orderDataLabels,
                datasets: [{
                    label: 'orders',
                    data: orderDataValues,
                    backgroundColor: [
                        'rgb(0, 128, 0)',
                        'rgb(255, 99, 132)',
                        'rgb(54, 162, 235)',
                        'rgb(255, 205, 86)',
                        'rgb(75, 192, 192)',
                        'rgb(153, 102, 255)',
                        'rgb(255, 159, 64)',
                        'rgb(255, 0, 0)',
                        'rgb(0, 255, 255)',
                        'rgb(128, 0, 128)'
                    ]
                }]
            },
            options: {
                scales: {
                    y: { beginAtZero: true }
                }
            }
        });




    </script>

