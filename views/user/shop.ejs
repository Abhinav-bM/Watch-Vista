      <script>
        // Function to fetch products by category
        function getProductsByCategory(category, event) {
          event.preventDefault(); // Prevent default behavior of anchor tag
          console.log("function called");
          axios
            .get(`/products/${category}`)
            .then((response) => {
              console.log(response);
              displayProducts(
                response.data.filteredProducts,
                response.data.user
              );
            })
            .catch((error) => {
              console.error(error);
            });
        }

        const shopSort = document.getElementById("shop-sort");
        shopSort.addEventListener("change", (event) => {
          const selectedOption = event.target.value;
          sortProducts(selectedOption);
        });

        function sortProducts(selectedOption) {
          console.log("selected option : ", selectedOption);
          axios
            .get(`/products/sort/${selectedOption}`)
            .then((response) => {
              console.log("lkjeflskdj :", response);
              displayProducts(response.data.allProducts, response.data.user);
            })
            .catch((error) => {
              console.error(error);
            });
        }

        // SEARCH FUNCTION
        document
          .getElementById("searchButton")
          .addEventListener("click", function () {
            const inputValue = document.getElementById("searchInput").value;

            axios
              .get(`/products/search/${inputValue}`)
              .then((response) => {
                console.log(response);
                displayProducts(
                  response.data.filteredProducts,
                  response.data.user
                );
                document.getElementById("searchInput").value = "";
              })
              .catch((error) => {
                console.error(error);
              });
          });

        // Function to display products on the page
        function displayProducts(products, user) {
          const productDisplay = document.getElementById("productDisplay");
          productDisplay.innerHTML = ""; // Clear existing products

          products.forEach((product) => {
            // Create elements for product components
            const productDiv = document.createElement("div");
            productDiv.classList.add(
              "col-xl-3",
              "col-lg-3",
              "col-md-4",
              "col-sm-6",
              "col-12"
            );

            const productBox = document.createElement("div");
            productBox.classList.add("product-box", "mb-40");

            const productBoxWrapper = document.createElement("div");
            productBoxWrapper.classList.add("product-box-wrapper");

            const productImg = document.createElement("div");
            productImg.classList.add("product-img");
            const img = document.createElement("img");
            img.src = product.productImages[0];
            img.classList.add("w-100");
            img.alt = product.productName;
            productImg.appendChild(img);

            const productDesc = document.createElement("div");
            productDesc.classList.add("product-desc", "pb-20");

            const productDescTop = document.createElement("div");
            productDescTop.classList.add("product-desc-top");

            const categories = document.createElement("div");
            categories.classList.add("categories");
            const categoryLink = document.createElement("a");
            categoryLink.href = `/products?productId=${product._id}`;
            categoryLink.classList.add("product-category");
            const categorySpan = document.createElement("span");
            categorySpan.textContent = product.productCategory;
            categoryLink.appendChild(categorySpan);
            categories.appendChild(categoryLink);

            const wishlistButton = document.createElement("button");
            wishlistButton.classList.add("btn", "btn-link");
            wishlistButton.onclick = function () {
              addToWishlist(event, product._id);
            };
            const wishlistDiv = document.createElement("div");
            wishlistDiv.classList.add("wishlist", "float-right");
            const wishlistIcon = document.createElement("i");
            wishlistIcon.id = `heartIcon-${product._id}`;
            wishlistIcon.classList.add("fal", "fa-heart", "fa-lg");
            wishlistDiv.appendChild(wishlistIcon);
            wishlistButton.appendChild(wishlistDiv);

            const productTitle = document.createElement("a");
            productTitle.href = `/products?productId=${product._id}`;
            productTitle.classList.add("product-title");
            productTitle.textContent = product.productName;

            const priceSwitcher = document.createElement("div");
            priceSwitcher.classList.add("price-switcher");
            const priceSpan1 = document.createElement("span");
            priceSpan1.classList.add("price", "switcher-item");
            priceSpan1.textContent = `₹ ${product.productPrice}`;
            const priceSpan2 = document.createElement("span");
            priceSpan2.classList.add("price", "switcher-item");
            priceSpan2.textContent = `₹ ${product.productPrice}`;

            // Assemble the product components
            productDescTop.appendChild(categories);
            productDescTop.appendChild(wishlistButton);
            productDesc.appendChild(productDescTop);
            productDesc.appendChild(productTitle);
            priceSwitcher.appendChild(priceSpan1);
            priceSwitcher.appendChild(priceSpan2);
            productDesc.appendChild(priceSwitcher);

            productBoxWrapper.appendChild(productImg);
            productBoxWrapper.appendChild(productDesc);
            productBox.appendChild(productBoxWrapper);
            productDiv.appendChild(productBox);

            // Append the product to the display container
            productDisplay.appendChild(productDiv);

            // Toggle heart icon based on wishlist status
            if (
              user &&
              user.wishlist.products.some(
                (wProduct) =>
                  wProduct.productId.toString() === product._id.toString()
              )
            ) {
              wishlistIcon.classList.add("text-danger");
            }
          });
        }

        // ADD TO WISHLIST
        function addToWishlist(event, productId) {
          event.preventDefault();
          axios
            .post("/products/addToWishlist", { productId })
            .then((response) => {
              console.log(response);
              // Get the corresponding heart icon element
              const heartIcon = document.getElementById(
                `heartIcon-${productId}`
              );
              // Check if the product was added or removed from wishlist
              if (
                response.data.message ===
                "Product added to wishlist successfully"
              ) {
                heartIcon.classList.remove("fal");
                heartIcon.classList.add("fas");
                heartIcon.classList.add("text-danger");
                Toastify({
                  text: "Product added to wishlist",
                  duration: 2000,
                  gravity: "top",
                  position: "right",
                  backgroundColor: "black",
                }).showToast();
              } else if (
                response.data.message ===
                "Product removed from wishlist successfully"
              ) {
                // Change heart icon to regular red heart
                heartIcon.classList.remove("fas");
                heartIcon.classList.remove("text-danger");
                heartIcon.classList.add("fal");
                Toastify({
                  text: "Product removed from wishlist",
                  duration: 2000,
                  gravity: "top",
                  position: "right",
                  backgroundColor: "black",
                }).showToast();
              }
            })
            .catch((error) => {
              console.log("sjfjskf", error);
              if (error.response.data.error === "User not found") {
                Toastify({
                  text: "Please login for wishlisting a product",
                  duration: 3000,
                  gravity: "top",
                  position: "right",
                  // backgroundColor: "black",
                }).showToast();
              }
            });
        }
      </script>

