    <script>
        // Function to add product to cart
        function addToCart(productId) {
            console.log("add to cart function triggered :",productId )
            axios.post('/cart/add-to-cart', {
                productId: productId
            })
            .then(function (response) {
                console.log("rrspnse :", response);
                
                if(response.status === 200){
                    Toastify({
                        text: "Product Added to cart",
                        duration: 2000,
                        gravity: "top",
                        position: "right",
                        backgroundColor: "black",
                    }).showToast();
                }
            })
            .catch(function (error) {
                if(error.response.status === 404){
                    Toastify({
                        text: "Try, After login",
                        duration: 2000,
                        gravity: "top",
                        position: "right",
                        backgroundColor: "red",
                    }).showToast();
                }else{
                    Toastify({
                        text: "An error occurred. Try again",
                        duration: 2000,
                        gravity: "top",
                        position: "right",
                        backgroundColor: "red",
                    }).showToast();
                }
            });
        }
    </script>














