    <script>
        function removeFromWishlist(productId) {
            console.log("slkfhdf :",productId);
            axios.delete(`/wishlist/remove/${productId}`)
            .then(response => {
                console.log(response.data);
                updateWishlistUI(productId);
            })
            .catch(error => {
                console.error(error);
            });
        }

        function updateWishlistUI(productId) {
            const wishlistItem = document.getElementById(`wishlistItem-${productId}`);
            if (wishlistItem) {
                wishlistItem.remove();
            }
        }

    </script>


